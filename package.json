{
  "name": "socketkit",
  "version": "0.6.0",
  "description": "Next-gen behavior analysis server with built-in encryption supporting HTTP2 and gRPC.",
  "homepage": "https://github.com/socketkit/socketkit-js#readme",
  "bugs": {
    "url": "https://github.com/socketkit/socketkit-js/issues",
    "email": "hello@socketkit.com"
  },
  "license": "MIT",
  "source": "index.js",
  "exports": "./dist/index.modern.js",
  "main": "./dist/index.cjs",
  "module": "./dist/index.module.js",
  "unpkg": "./dist/index.umd.js",
  "type": "module",
  "scripts": {
    "build": "microbundle",
    "test": "c8 ava",
    "lint": "eslint \"**/*.js\" --fix",
    "prepublishOnly": "microbundle"
  },
  "keywords": [
    "encryption",
    "http2",
    "behavior",
    "microservice",
    "security"
  ],
  "author": "Yagiz Nizipli <yagiz@nizipli.com>",
  "dependencies": {
    "@socketkit/ajv-currency-code": "^1.0.1",
    "@socketkit/ajv-locale-code": "^1.0.1",
    "@socketkit/ajv-semver": "^1.0.1",
    "ajv": "^8.6.2",
    "undici": "^4.2.1"
  },
  "devDependencies": {
    "@babel/eslint-parser": "^7.14.7",
    "@babel/plugin-syntax-class-properties": "^7.12.13",
    "@babel/preset-env": "^7.14.8",
    "ava": "^3.15.0",
    "c8": "^7.7.3",
    "eslint": "^7.31.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.23.4",
    "eslint-plugin-json": "^3.0.0",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-plugin-security": "^1.4.0",
    "microbundle": "^0.13.3",
    "prettier": "^2.3.2"
  },
  "publishConfig": {
    "source": "index.js",
    "main": "dist/index.js"
  },
  "ava": {
    "files": [
      "./**/*.test.js"
    ],
    "failFast": true,
    "nodeArguments": [
      "--no-warnings",
      "--experimental-json-modules"
    ]
  },
  "eslintConfig": {
    "env": {
      "node": true,
      "es6": true
    },
    "extends": [
      "eslint:recommended",
      "plugin:prettier/recommended",
      "plugin:import/recommended",
      "plugin:security/recommended",
      "plugin:json/recommended"
    ],
    "ignorePatterns": [
      ".github",
      "coverage",
      "dist"
    ],
    "parser": "@babel/eslint-parser",
    "parserOptions": {
      "babelOptions": {
        "configFile": "./.babelrc"
      },
      "ecmaVersion": 2021
    },
    "plugins": [
      "prettier",
      "import",
      "security"
    ],
    "rules": {
      "eqeqeq": [
        "error",
        "always"
      ]
    },
    "settings": {
      "jsdoc": {
        "mode": "typescript"
      }
    }
  }
}
