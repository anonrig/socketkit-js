{
  "name": "socketkit",
  "version": "0.6.0",
  "description": "Next-gen behavior analysis server with built-in encryption supporting HTTP2 and gRPC.",
  "homepage": "https://github.com/socketkit/socketkit-js#readme",
  "bugs": {
    "url": "https://github.com/socketkit/socketkit-js/issues",
    "email": "hello@socketkit.com"
  },
  "license": "MIT",
  "source": "index.js",
  "exports": {
    "require": "./dist/index.cjs",
    "default": "./dist/index.modern.js"
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.module.js",
  "unpkg": "./dist/index.umd.js",
  "type": "module",
  "scripts": {
    "build": "microbundle",
    "test": "c8 ava",
    "lint": "eslint \"**/*.js\" --fix",
    "quickstart-node": "microbundle && node ./quickstart-example-node.js",
    "prepublishOnly": "microbundle"
  },
  "keywords": [
    "encryption",
    "http2",
    "behavior",
    "microservice",
    "security"
  ],
  "author": "Yagiz Nizipli <yagiz@nizipli.com>",
  "dependencies": {
    "axios": "^0.24.0",
    "isomorphic-webcrypto": "^2.3.8"
  },
  "devDependencies": {
    "@anonrig/eslint-config": "^0.2.1",
    "@babel/core": "^7.16.0",
    "@babel/eslint-parser": "^7.16.3",
    "@babel/plugin-syntax-class-properties": "^7.12.13",
    "@babel/preset-env": "^7.16.4",
    "ava": "^3.15.0",
    "c8": "^7.10.0",
    "eslint": "^8.3.0",
    "microbundle": "^0.14.2"
  },
  "publishConfig": {
    "source": "index.js",
    "main": "dist/index.js"
  },
  "ava": {
    "files": [
      "./**/*.test.js"
    ],
    "failFast": true,
    "nodeArguments": [
      "--no-warnings",
      "--experimental-json-modules"
    ]
  },
  "eslintConfig": {
    "extends": [
      "@anonrig/eslint-config"
    ],
    "parser": "@babel/eslint-parser",
    "parserOptions": {
      "babelOptions": {
        "configFile": "./.babelrc"
      },
      "ecmaVersion": "latest"
    }
  }
}
